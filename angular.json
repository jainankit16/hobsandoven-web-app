{
  "$schema": "./node_modules/@angular-devkit/core/src/workspace/workspace-schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "community": {
      "root": "",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist-community",
            "index": "src/index.html",
            "main": "src/main.community.ts",
            "tsConfig": "src/tsconfig.community.json",
            "polyfills": "src/polyfills.ts",
            "assets": [
              {
                "glob": "favicon.ico",
                "input": "src/",
                "output": "/"
              },
              {
                "glob": "**/*",
                "input": "src/assets",
                "output": "/assets"
              }
            ],
            "styles": [
              "./src/assets/plugins/bootstrap/css/bootstrap.min.css",
              "./src/assets/css/style.css",
              "./src/assets/css/colors/blue-dark.css",
              "./src/assets/css/custom.css",
              "./src/assets/css/upload.css",
              "./node_modules/ng-pick-datetime/assets/style/picker.min.css",
              "./src/styles.css",
              "./src/assets/css/ngselect/material.theme.css",
              "./node_modules/quill/dist/quill.core.css",
              "./node_modules/quill/dist/quill.bubble.css",
              "./node_modules/quill/dist/quill.snow.css",
              "./src/assets/css/grid.view.css"
            ],
            "scripts": [
              "./src/assets/plugins/jquery/jquery.min.js",
              "./src/assets/plugins/popper/popper.min.js",
              "./src/assets/plugins/bootstrap/js/bootstrap.min.js",
              "./src/assets/js/jquery.slimscroll.js",
              "./src/assets/plugins/sparkline/jquery.sparkline.min.js",
              "./src/assets/js/pace.min.js",
              "./src/assets/plugins/styleswitcher/jQuery.style.switcher.js",
              "./src/assets/js/sidebarmenu.js",
              "./src/assets/js/freezeheader.js",
              "./node_modules/moment/min/moment.min.js"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "./src/environments/environment.ts",
                  "with": "./src/environments/environment.production.ts"
                }
              ]
            },
            "staging": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "./src/environments/environment.ts",
                  "with": "./src/environments/environment.staging.ts"
                }
              ]
            },
            "development": {
              "fileReplacements": [
                {
                  "replace": "./src/environments/environment.ts",
                  "with": "./src/environments/environment.development.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "community:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "community:build:production"
            },
            "staging": {
              "browserTarget": "community:build:staging"
            },
            "development": {
              "browserTarget": "community:build:development"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "community:build"
          }
        }
      }
    },
